{
	"manifest_version": 2,
	"name": "Ace Plugin",
	"description": "This extension shows an Ace editor in all TEXTAREA fields with json",
	"version": "2.0",
	"permissions": [
	    "*://*/i2/dashboard"
	  ],
	"content_scripts": [
		{
			"matches": ["*://*/*"],
			"js": ["patch-worker.js", "ace/ace.js","ace/mode-json.js","ace/theme-github.js","plugin.js"]
		}
	],
	"web_accessible_resources": [
		"ace/worker-json.js"
	]
}


